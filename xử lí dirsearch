# lấy các url trong list url 
# tim các địa chỉ có login
# đưa đưa ra output là file json
# lấy được các url trong file json là các url login
# tạo thanh 1 list cac url login để đưa vào acunetix
import json
import os,sys
import subprocess
list_url=[]
f = open("aquatone_urls.txt", "r+")
for x in f:
    list_url.append(x)
f.close()

for x in list_url:
    print(x)
list_url.append(os.system(f"python3 /home/longlagon/dirsearch/dirsearch.py -e php, html, js -u http://testphp.vulnweb.com/ --prefixes login  --format html -o {'4'}.json"))
print(list_url)

with open("/home/longlagon/4.json", "r") as fin:
    data = json.loads(fin.read())
print(data) # lỗi ở file json để lay được url 
